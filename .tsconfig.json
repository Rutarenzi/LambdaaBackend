{
    "compilerOptions": {
     "target": "es6",
     "module":"commonjs",
     "outDir": "./build",
     "rootDir":"./src",
     "strict": true,
     "esModuleInterop": true,
     "skipLibCheck":true,
     "forceConsistentCasingInFileNames": true,
     "resolveJsonModule":true,
     "types": ["sequelize", "sequelize-typescript"]
    },
    "include":["src/**/*.ts"],
    "exclude":["node_modules"] 
 }